#!/usr/bin/env groovy

pipeline {
    agent any
    stages {
        stage("Start") {
            steps {
                script {
                        echo "Pipeline started"
                    }
                }
        }
        stage("Building") {
            steps {
                script {
                        echo "Building"
                    }
                }
        }
        stage("Done") {
            steps {
                script {
                        echo "Job is done"
                    }
                }
        }
     }

}
